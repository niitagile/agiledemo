<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="q1" class="com.DI.PropertyExample.Question">
<property name="q_no" value="1"></property>  
<property name="ques" value="What is Java?"></property>  
<property name="answers">  
<list>  
<value>Java is a programming language</value>  
<value>Java is a platform</value>  
<value>Java is an Island</value>  
</list>  
</property>
</bean>
<bean id="q2" class="com.DI.PropertyExample.Question">
<property name="q_no" value="2"></property>  
<property name="ques" value="What is Spring?"></property>  
<property name="answers">  
 <list>
<value>Spring is a programming language</value>
<value>Spring is a framework</value>
<value>Spring is a hardware</value>
</list>
</property>
</bean>

</beans>